{
  "name": "pwa-react-boilerplate",
  "version": "0.1.5",
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "history": "^3.0.0",
    "material-ui": "^0.16.7",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "rm -rf build && webpack --env.prod --progress --profile",
    "gh": "yarn run build -- --env.prefix $npm_package_name && gh-pages -d build",
    "test": "xo && NODE_ENV=test ava --verbose --serial",
    "lint": "xo"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-react-app": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "0.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.26.0",
    "minimist": "^1.2.0",
    "react-addons-test-utils": "^15.4.2",
    "style-loader": "0.13.1",
    "sw-precache": "^4.3.0",
    "sw-precache-webpack-dev-plugin": "^0.1.3",
    "sw-precache-webpack-plugin": "^0.7.2",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9",
    "xo": "^0.17.1"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "extends": [
      "xo",
      "xo-react"
    ],
    "rules": {
      "quote-props": [
        "error",
        "as-needed"
      ],
      "react/require-optimization": 0,
      "react/forbid-component-props": 0,
      "import/no-unresolved": 0,
      "import/no-unassigned-import": 0,
      "unicorn/filename-case": 0
    }
  },
  "ava": {
    "files": "test/*.js",
    "require": "babel-register",
    "babel": "inherit"
  }
}
